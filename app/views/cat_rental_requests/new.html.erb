<form action= <%= cat_rental_requests_url %> method="post">

  <h1>Cat Rental Request</h1>
  <br>
  <p>Select your desired cat!</p>
  <select name="cat_rental_request[cat_id]">
    <option>Cat</option>
    <% Cat.all.each do |cat| %>
      <option value="<%= cat.id %>"><%= cat.name %></option>
     <% end %>
  </select>
  <br>
  <br>
  <p>Select start date and end date</p>
    <input type="date" name="cat_rental_request[start_date]" value= <%= @cat_rental_request.start_date %> >
    <input type="date" name="cat_rental_request[end_date]" value= <%= @cat_rental_request.end_date %> >
  </p>
  <p>
    <input type="submit" value="Submit">
  </p>
</form>
